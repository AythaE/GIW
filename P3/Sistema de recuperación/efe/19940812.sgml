    Cataluña           0,3            2,1             4,4
    C. Valenciana      0,6            2,3             4,2
    Extremadura        0,4            3,2             5,4
    Galicia            0,4            2,8             5,0
    Madrid             0,2            2,1             4,7
    Murcia             0,6            3,4             5,0
    Navarra            0,6            2,8             5,6
    País Vasco         0,4            2,4             5,0
    La Rioja           0,5            2,4             5,2
    ---------------------------------------------------------
    Ceuta-Melilla      0,1            2,3             5,0
    =========================================================
    INDICE GENERAL     0,4            2,5             4,8
    EFE
